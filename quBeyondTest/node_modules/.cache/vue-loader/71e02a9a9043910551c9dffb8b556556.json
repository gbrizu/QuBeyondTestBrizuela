ow.orbital_period }}</td>                \n                <td>{{ row.diameter }}</td>                \n                <td>{{ row.climate }}</td>                \n                <td>{{ row.gravity }}</td>                \n                <td>{{ row.terrain }}</td>                \n                <td>{{ row.surface_water }}</td>                \n                <td>{{ row.population }}</td>                \n                <td>\n                    <button> Ver </button>\n                </td>                \n                </tr>\n            </tbody>\n        </v-table>\n         <div>\n             <button v-on:click=\"handlePrevious()\">Anterior</button>\n             [{{currentPage}}]\n             <button v-on:click=\"handleNext()\">Siguiente</button>\n         </div>\n       \n    </div>\n</div>    \n</template>\n\n<script>\nimport Vue from 'vue';\n import axios from 'axios';\n import SmartTable from 'vuejs-smart-table';\n Vue.use(SmartTable)\n\nexport default {          \n    data(){\n        return{\n            currentPage: 1,\n            planets:[],\n            previuosUrl:null,\n            nextUrl:null,\n            initialUrl:'https://swapi.dev/api/planets/'\n        }    \n    },\n\n    mounted(){\n        this.getPlanets(this.initialUrl);\n    },\n\n    methods:{\n    getPlanets(url){\n      \n     axios\n        .get(url)        \n        .then(response => {\n            this.planets = response.data.results;            \n            this.nextUrl = response.data.next;\n            this.previuosUrl = response.data.previous;\n            console.log(response.data);\n        }).\n        catch(e => {\n            console.log(e);\n        })\n    },\n\n    handlePrevious(){\n        console.log('entro a la fincion');\n        if(this.previuosUrl != null){\n            this.currentPage--;\n            console.log(this.previuosUrl);\n            this.getPlanets(this.previuosUrl)\n        }        \n    },\n    handleNext(){\n        console.log('entro a la fincion');\n        console.log(this.nextUrl);\n        if(this.nextUrl != null){\n            this.currentPage++;\n            console.log(this.nextUrl);\n            this.getPlanets(this.nextUrl)\n        } \n    },\n}\n}\n</script>\n\n<style>\n\n</style>\n"]}]}